public class YourInitials{
    public static void main (String[] args){
        System.out.println("PPPPP   IIIIIII    CCC  ");
        System.out.println("P    P     I      C   C ");
        System.out.println("P    P     I     C      ");
        System.out.println("PPPPP      I     C      ");
        System.out.println("P          I     C      ");
        System.out.println("P          I      C   C ");
        System.out.println("P       IIIIIII    CCC  ");
    }
}
